{% extends 'base.html.twig' %}

{% block title %}Enregistrez vos boutiques{% endblock %}

{% block body %}

<h1>Enregistrez vos boutiques</h1>
	
	<div class="container-fluid">
		<div class="row">
			<div class="col-12 col-md-6 mx-auto text-center mt-5">
				{{ form_start(createShopForm) }}
                {{ form_widget(createShopForm.owner, {
                    'attr' : {
                        'class' : 'd-none'
                    }
                })
                }}
                {{ form_end(createShopForm) }}
                
			</div>
		</div>
	</div>
	
{% endblock %}
{% block jsIncludes %}
	<script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
	<script src="{{ asset('js/typeahead/typeahead.js') }}"></script>
	<script src="{{ asset('js/typeahead/autocompletion-shop-inputs.js') }}"></script>
{% endblock %}


