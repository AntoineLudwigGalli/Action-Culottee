{% extends 'baseAdmin.html.twig' %}

{% block title %}Créer un nouvel évènement{% endblock %}

{% block content %}

    <div class="container-fluid">
        <div class="col-md-10 offset-md-2">
            <a href="{{ path('admin_panel_events_list', {'id': form.vars.value.id}) }}"
               class="text-decoration-none py-5">Revenir sur la liste</a>
            <h1>Créer un nouvel évènement</h1>
            <div class="row">
                <div class="col-12 col-md-6 mx-auto text-center mt-5">
                    {{ form(form) }}
                </div>
            </div>
        </div>
    </div>


{% endblock %}


